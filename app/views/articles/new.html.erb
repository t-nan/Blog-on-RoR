

<div class="container">

<h2>Add new article</h2>

<%= form_for :article,url: articles_path do |f| %>

<p>
	<%= f.label :title %></br>
	<%= f.text_field :title %>
</p>

<p>
	<%= f.label :text %></br>
	<%= f.text_field :text %>
</p>

<p>
	<%= f.submit 'Save article' %></br>
	
</p>

<% end %>

</div>




